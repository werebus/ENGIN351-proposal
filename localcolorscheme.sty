\setbeamercolor*{palette secondary}{fg=schemebase!70!black,bg=white!60!schemebase}
\setbeamercolor*{palette tertiary}{bg=schemebase!80!black,fg=white!50!schemebase}
\setbeamercolor*{palette quaternary}{fg=schemebase,bg=white!20!schemebase}

\setbeamercolor*{sidebar}{fg=schemebase,bg=schemebase!75!white}

\setbeamercolor*{navigation symbols dimmed}{fg=lightgray}

\setbeamercolor*{palette sidebar primary}{fg=schemebase!10!black}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{fg=schemebase!50!black}
\setbeamercolor*{palette sidebar quaternary}{fg=white!10!schemebase}

\setbeamercolor*{titlelike}{parent=palette primary}
\setbeamercolor{frametitle}{bg=white!90!schemebase}
\setbeamercolor{frametitle right}{bg=white!60!schemebase}

\setbeamercolor{subsection in head/foot}{fg=schemebase!10!white}
